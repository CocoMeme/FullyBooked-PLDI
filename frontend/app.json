{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "fullybooked",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fullybooked.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.fullybooked.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      [
        "@react-native-google-signin/google-signin",
        {
          "webClientId": "965289265275-00crng1jcruvnq9cfk51ls30qs0tt4vt.apps.googleusercontent.com",
          "iosUrlScheme": "com.googleusercontent.apps.965289265275-00crng1jcruvnq9cfk51ls30qs0tt4vt"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ad384908-c562-427e-b8eb-a790a6957083"
      }
    },
    "entryPoint": "./App.js"
  }
}
